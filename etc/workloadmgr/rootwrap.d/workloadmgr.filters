# workloadmgr-rootwrap command filters for workloads nodes
# This file should be owned by (and only-writeable by) the root user

[Filters]
virsh: CommandFilter, /usr/bin/virsh, root
tee: CommandFilter, /usr/bin/tee, root
chown: CommandFilter, /bin/chown, root
qemu-img: CommandFilter, /usr/bin/qemu-img, root
vmware-vdiskmanager: CommandFilter, /usr/bin/vmware-vdiskmanager, root
chmod: CommandFilter, /bin/chmod, root
ssh: CommandFilter, /usr/bin/ssh, root
scp: CommandFilter, /usr/bin/scp, root
gcp: CommandFilter, /usr/bin/gcp, root
trilio-vix-disk-cli: CommandFilter, /usr/bin/trilio-vix-disk-cli, root
